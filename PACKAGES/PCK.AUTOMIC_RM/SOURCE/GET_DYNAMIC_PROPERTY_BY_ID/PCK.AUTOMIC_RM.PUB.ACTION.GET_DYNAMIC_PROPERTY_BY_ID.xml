:ENDPROCESS

:IF &STATUS_OK# = 'TRUE'
:  SET &CHECKFAIL_MISSING# = PREP_PROCESS_REPORT(,,,"*Error from the execution of the query*")
:  PROCESS &CHECKFAIL_MISSING#
:    PRINT "Dynamic Property &UC4RB_DYNAMIC_FULL_NAME# does not exist, Fail If Missing is set to &UC4RB_FAIL_MISSING#"
:    IF &UC4RB_FAIL_MISSING# = 'NO'
:      MODIFY_STATE STATUS_TEXT = 'Dynamic Property does not exist'
:      MODIFY_STATE RETCODE = 0
:    ENDIF
:  ENDPROCESS
:ENDIF]]></post_process>
      </row>

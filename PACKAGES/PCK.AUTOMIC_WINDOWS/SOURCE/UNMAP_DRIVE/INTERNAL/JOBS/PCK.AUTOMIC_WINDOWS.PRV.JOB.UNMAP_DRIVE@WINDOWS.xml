   <scripts>
      <row>
         <process><![CDATA[! run book function to unmap network shared drive
!
!

:IF &UC4RB_DEVICE_NAME# = ""
   GOTO ABEND
:ENDIF
@echo off
"%SYSTEMROOT%\system32\net.exe" use &UC4RB_DEVICE_NAME# /DELETE
set returncode=%errorlevel%
if %returncode% EQU 0 (
echo Share unmapped successfully
) else (
echo Error occurred whilst unmapping share
exit /b %returncode%
)

dir &UC4RB_DEVICE_NAME#\
set returncode=%errorlevel%
if %returncode% EQU 1 (
echo Check share unmapped passed
) else (
echo Error occurred whilst checking share unmapped
exit /b 1
)
exit /b 0
:INCLUDE PCK.ITPA_SHARED.PRV.INCLUDE.CHECK_CMDLINE_CMD@WINDOWS]]></process>
      </row>

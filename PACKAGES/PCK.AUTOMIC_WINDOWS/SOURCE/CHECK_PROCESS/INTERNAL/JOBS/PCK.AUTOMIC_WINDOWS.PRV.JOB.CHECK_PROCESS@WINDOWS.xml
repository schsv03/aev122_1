         <queue>CLIENT_QUEUE</queue>
         <versioning_id>689384403</versioning_id>
      </row>
   </general_attributes>
   <scripts>
      <row>
         <process><![CDATA[!Check if process is running
!

@echo off
tasklist /nh /fi "imagename eq &UC4RB_PROCESS_NAME#" | findstr /i "&UC4RB_PROCESS_NAME#" >nul
IF ERRORLEVEL 1 ( echo &UC4RB_PROCESS_NAME# is not running ) ELSE ( echo &UC4RB_PROCESS_NAME# is running )

:INCLUDE PCK.ITPA_SHARED.PRV.INCLUDE.CHECK_CMDLINE_CMD@WINDOWS]]></process>
      </row>
      <row>
         <pre_process>:INCLUDE PCK.ITPA_SHARED.PRV.INCLUDE.PREPARE_JOB</pre_process>
      </row>
      <row>
         <post_process><![CDATA[:SET &HND# = PREP_PROCESS_REPORT("JOBS",,"REP","*running*")
:PROCESS &HND#
:  SET &RET# = GET_PROCESS_LINE(&HND#)
:  P &RET#
:  P "&UC4RB_PROCESS_NAME# is not running"
:  IF &RET# = "&UC4RB_PROCESS_NAME# is not running"
:    PSET &UC4RB_PROCESS_STATE# = "N"
:  ENDIF
:  IF &RET# = "&UC4RB_PROCESS_NAME# is running"
:    PSET &UC4RB_PROCESS_STATE# = "Y"
:  ENDIF
:ENDPROCESS
:P "&UC4RB_PROCESS_NAME# is running: &UC4RB_PROCESS_STATE#"
:CLOSE_PROCESS &HND#]]></post_process>
      </row>
   </scripts>

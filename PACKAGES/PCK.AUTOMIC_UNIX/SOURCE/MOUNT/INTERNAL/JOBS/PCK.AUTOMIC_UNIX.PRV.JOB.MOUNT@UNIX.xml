   <scripts>
      <row>
         <process><![CDATA[:INCLUDE PCK.ITPA_SHARED.PRV.INCLUDE.SUDO_EXECUTION

:if &UC4RB_UNIX_TYPE# <> ""
  case `uname -s` in
    HP-UX | SunOS) opt=-F ;;
    *) opt=-t ;;
  esac
  type="$opt &UC4RB_UNIX_TYPE#"
:else
  type=
:endif

:if &UC4RB_UNIX_RO# = "YES"
  ro="-o ro"
:else
  ro=
:endif

mount $type $ro &UC4RB_UNIX_ARGS# &UC4RB_UNIX_DEVICE# &UC4RB_UNIX_DIRECTORY#
:INCLUDE PCK.ITPA_SHARED.PRV.INCLUDE.CHECK_SHELL_CMD@UNIX]]></process>

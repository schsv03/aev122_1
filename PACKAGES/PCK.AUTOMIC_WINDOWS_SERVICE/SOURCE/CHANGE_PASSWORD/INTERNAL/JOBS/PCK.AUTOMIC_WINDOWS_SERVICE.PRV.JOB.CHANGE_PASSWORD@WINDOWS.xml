      <row>
         <process><![CDATA[:INCLUDE PCK.ITPA_SHARED.PUB.INCLUDE.ATTACH
:INCLUDE PCK.ITPA_SHARED.PRV.INCLUDE.CHECK_CMDLINE_CMD@WINDOWS

! Update service password
! Check if service exists
%SYSTEMROOT%\system32\sc.exe query "&UC4RB_SCM_SERVICE_NAME#" | findstr "&UC4RB_SCM_SERVICE_NAME#"
if %errorlevel% NEQ 0 exit /b %errorlevel%

! Change service password
java -jar "&$AGENT_RESOURCES_CLIENT#/itpa-tool.jar" ARB -cmd execute %SYSTEMROOT%\system32\sc.exe config "&UC4RB_SCM_SERVICE_NAME#" password= "&UC4RB_SCM_SERVICE_PASSWORD#"
if %errorlevel% NEQ 0 exit /b 1]]></process>
      </row>
      <row>
         <pre_process>:INCLUDE PCK.ITPA_SHARED.PRV.INCLUDE.PREPARE_JOB</pre_process>
      </row>

   <scripts>
      <row>
         <pre_process><![CDATA[:INC PCK.AUTOMIC_RA_REST.PUB.INCLUDE.PREPARE_RA_JOB
:INC PCK.AUTOMIC_RA_REST.PUB.INCLUDE.SET_CONNECTION_AUTH_NONE]]></pre_process>
      </row>
      <row>
         <post_process><![CDATA[:D &ARR#,string,100
:D &ARR1#,string,2
:FILL &ARR#[] = STR_SPLIT(&CUSTOM_PROPERTIES#[1],",")

:SET &RUNVAR# = 1
:SET &LEN# = LENGTH(&ARR#[])

:WHILE &RUNVAR# LE &LEN#
:  SET &PORT_VALUE# = STR_SUB(&ARR#[&RUNVAR#],'"')
:  SET &PORT_VALUE# = STR_SUB(&PORT_VALUE#,'}')
:  SET &PORT_VALUE# = STR_SUB(&PORT_VALUE#,'{',)
:  SET &PORT_VALUE# = STR_TRIM(&PORT_VALUE#)
:  SET &MATCH_PORT# = STR_MATCH(&PORT_VALUE#,"*port*:*")
:  SET &INVALID_FORM# = STR_MATCH(&PORT_VALUE#,"*:*:*")
:  IF &MATCH_PORT# = "Y"
:    IF &INVALID_FORM# = "N"
:      FILL &ARR1#[] = STR_SPLIT(&PORT_VALUE#,":")
:      SET &ISNUM# = ISNUMERIC(&ARR1#[2])
:      IF &ISNUM# = "Y"
:        PUT_VAR &VARA_TARGET_NAME#, "&ARR1#[1]","&ARR1#[2]"
:      ENDIF
:    ENDIF
:  ENDIF
:  SET &RUNVAR# = &RUNVAR# + 1
:ENDWHILE]]></post_process>
      </row>
   </scripts>
